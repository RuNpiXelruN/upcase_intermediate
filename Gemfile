source 'https://rubygems.org'

gem 'rails', '4.2.6'

gem 'sqlite3'

gem 'devise'

group :assets do
  gem 'sass-rails', '~> 5.0'
  gem 'uglifier', '>= 1.3.0'
  gem 'coffee-rails', '~> 4.1.0'
end

gem 'paperclip'

